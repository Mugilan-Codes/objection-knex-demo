{
  "name": "objection-knex",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "down": "knex --knexfile ./src/db/knexfile.js --esm migrate:down",
    "rollback": "knex --knexfile ./src/db/knexfile.js --esm migrate:rollback",
    "migrate": "knex --knexfile ./src/db/knexfile.js --esm migrate:latest",
    "seed": "knex --knexfile ./src/db/knexfile.js --esm seed:run",
    "nodemon": "nodemon --exec babel-node ./src",
    "dev": "npm run nodemon",
    "start:dev": "npm run migrate && npm run seed && npm run nodemon",
    "clean": "rm -rf ./dist",
    "build:babel": "babel ./src --out-dir dist --copy-files --source-maps",
    "build": "npm run clean && npm run build:babel",
    "start": "npm run build && node ./dist",
    "start:prod": "node ./dist"
  },
  "keywords": [],
  "author": "Mugilan <mugilancodes@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "express": "^4.17.1",
    "knex": "^0.21.19",
    "mysql2": "^2.2.5",
    "objection": "^2.2.15"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "nodemon": "^2.0.7"
  }
}
